@model Branche

<p>
    ARE YOU SURE YOU WANT TO DELETE @Model.Name BRANCHE ?
</p>
<p>
    <a class="btn btn-primary text-white" asp-action="Index"> Cancel</a>
    
    <form method="post" class="d-inline" asp-action="ConfirmDelete" asp-controller="Branches" asp-route-id="@Model.Id">
        <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
</p>