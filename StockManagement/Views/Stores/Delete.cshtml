@model Store

<p>
    ARE YOU SURE YOU WANT TO DELETE @Model.Name STORE ?
</p>
<p>

    <form method="post" class="d-inline" asp-action="ConfirmDelete" asp-controller="Stores"
          asp-route-id="@Model.Id" asp-route-BrancheId="@Model.BrancheId">
        <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
</p>
