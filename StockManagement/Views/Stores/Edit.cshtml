@model Store

<h1>UPDATE STORE</h1>
<div class="container">
    <form method="post"  asp-action="Update" asp-controller="Stores" enctype="multipart/form-data">
        <div class="row">
            <input type="hidden" asp-for="Id" />
            <div class="col-6">
                <label>
                    @Html.DisplayNameFor(x => x.Name)
                </label>
                <input type="text" asp-for="Name" class="form-control" />
            </div>
            
            <input type="hidden" asp-for="BrancheId" />
            <div class="col-6">
                <input type="submit" class="btn btn-success" value="save" />
            </div>

        </div>
    </form>
</div>
